{
	"name": "music-dj-extender",
	"version": "1.0.0",
	"type": "module",
	"license": "MIT",
	"scripts": {
		"dev": "cross-env NODE_ENV=development tsx ./server/index.ts",
		"build": "vite build && esbuild ./server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
		"start": "NODE_ENV=production node dist/index.js",
		"check": "tsc",
		"db:push": "drizzle-kit push",
		"setup": "npm install && mkdir -p uploads results",
		"clean": "rimraf dist uploads/* results/*",
		"db:test": "node scripts/test-db-connection.js",
		"start:dev": "node scripts/start-app.bat",
		"analyze:bundle": "node scripts/bundle-size-monitor.js",
		"build:analyze": "npm run build && npm run analyze:bundle",
		"build:stats": "vite build --mode production && npx vite-bundle-analyzer dist/public/assets/*.js",
		"lint": "eslint . --ext .js,.ts,.tsx",
		"lint:fix": "eslint . --ext .js,.ts,.tsx --fix",
		"lint:security": "eslint . --ext .js,.ts,.tsx --config eslint.config.js | grep -i security",
		"lint:report": "eslint . --ext .js,.ts,.tsx --format json --output-file eslint-report.json"
	},
